<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

				<context:property-placeholder location="inof.properties"/>

		<bean id="emp" class="com.cjc.model.Employee">
		<property name="eid" value="${employee.eid}"/>
		<property name="name" value="${employee.name}"/>
		<property name="desig" value="${employee.desig}"/>
		</bean>
		
		<bean id="emp1" class="com.cjc.model.Employee">
		<property name="eid" value="${employee1.eid}"/>
		<property name="name" value="${employee1.name}"/>
		<property name="desig" value="${employee1.desig}"/>
		</bean>


</beans>
